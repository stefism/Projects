<div>
    <h1 class="display-4">Числа</h1>
    <h1>@ViewBag.Nums</h1>
    <p>Проба 123</p>

    <ul class="nums">

        @if (ViewBag.Nums != null && ViewBag.Nums.Count > 0)
        {
            @foreach (var num in ViewBag.Nums)
            {
                <li style="color: yellow; font-size: 30px">@num</li>
            }
        }
        else
        {
            <div style="color: yellow; font-size: 30px">Няма ги числата</div>
        }
    </ul>

</div>

