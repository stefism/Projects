@model IEnumerable<Funeral.App.ViewModels.AllFramesViewModel>
<!DOCTYPE html>
<html>
<head>
    <title>Направи си</title>
    <script src="https://kit.fontawesome.com/3c54b527d8.js" crossorigin="anonymous"></script>
    <link rel="stylesheet" href="~/css/Main.css">
</head>
<body>
    <aside id="menu-items">

        <ul>
            <li><a href="#frames">рамки</a></li>
            <li><a href="#texts">текстове</a></li>
            <li><a href="#crosses">кръстове</a></li>
        </ul>

    </aside>

    <section id="design">
        <div id="main-window">
            <img id="background" src="~/Pictures/Frames/1.gif" alt="">
        </div>
        <div id="items">

            @foreach (var frame in Model)
            {
                <img class="elements" src="@frame.FilePath" alt="">
            }

        </div>
    </section>
</body>
</html>